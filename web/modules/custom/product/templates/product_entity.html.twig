{#
/**
 * @file advertising_entity.html.twig
 * Default theme implementation to present Advertising entity data.
 *
 * This template is used when viewing Advertising entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_advertising_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('advertising_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
